<% @blogs.each do |blog| %>
    <p><%= blog.title %></p>
    <p><%= link_to blog.url, "http://" + blog.url %></p>
    <span><%= link_to "Info", blog_path(blog) %></span> |     
    <span><%= link_to "Edit", edit_blog_path(blog) %></span> | 
    <span><%= link_to "Delete", blog_path(blog), method: :delete %></span>
<% end %>
